{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@LOY/*": ["src/_LOY/*"],
			"@PNC/*": ["src/_PNC/*"],
			"@SNB/*": ["src/_SNB/*"],
			"@utils/*": ["src/utils/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
